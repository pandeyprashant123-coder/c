#include <stdio.h>

int main() {
  float a,b,c,percentage;
  printf("Enter the marks for:\nMaths:\n");
  scanf("%f",&a);
  printf("Science:\n");
  scanf("%f",&b);
  printf("Nepali:\n");
  scanf("%f",&c);
  percentage = (a + b + c)/3;
  if(a>33 && b>33 && c>33 && percentage>=40){
      printf("You are pass with percentage %f\nCongrulation!!",percentage);
  }
  else{
    printf("You are Fail with percentage %f\nBetter luck Next Time.",percentage);
  }
  return 0;
}
